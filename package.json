{
  "name": "deliverybot",
  "version": "0.1.7",
  "main": "lib/index.js",
  "files": [
    "lib/*",
    "views/*",
    "static/*",
    "app.yml"
  ],
  "description": "Controls and configures deployments using the GitHub API.",
  "author": "Deliverybot <support@deliverybot.dev> (https://github.com/deliverybot/deliverybot)",
  "license": "MIT",
  "repository": "https://github.com/deliverybot/deliverybot.git",
  "homepage": "https://deliverybot.dev",
  "documentation": "https://deliverybot.dev/docs/",
  "bugs": "https://github.com/deliverybot/deliverybot/issues",
  "community": "https://spectrum.chat/deliverybot/general",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && node src/partials.js > static/partials.js",
    "dev": "nodemon --watch src --exec \"npm start\"",
    "start": "npm run build && NODE_ENV=development probot run ./lib/index.js",
    "format": "prettier --write \"./**/*.ts\"",
    "test": "LOG_LEVEL=fatal jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "prepack": "npm run build",
    "release": "standard-version",
    "finish": "git push --follow-tags origin master && npm publish"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^0.31.0",
    "@octokit/graphql": "^3.0.1",
    "body-parser": "1.19.0",
    "client-sessions": "^0.8.0",
    "csurf": "^1.10.0",
    "express": "4.16.4",
    "express-async-errors": "^3.1.1",
    "handlebars": "4.1.2",
    "js-yaml": "^3.13.1",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mustache": "^3.0.3",
    "node-uuid": "^1.4.8",
    "object-hash": "^1.3.1",
    "probot": "9.2.19",
    "request": "2.88.0",
    "turbolinks": "^5.2.0",
    "turbolinks-express": "^1.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/client-sessions": "^0.8.0",
    "@types/csurf": "^1.9.35",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/jsonschema": "^1.1.1",
    "@types/lodash": "^4.14.139",
    "@types/mustache": "^0.8.32",
    "@types/nock": "^9.3.0",
    "@types/node": "^11.9.0",
    "@types/node-uuid": "0.0.28",
    "@types/object-hash": "^1.3.0",
    "@types/request": "^2.48.2",
    "@types/turbolinks": "^5.2.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.0.0",
    "jest-raw-loader": "^1.0.1",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "prettier": "^1.17.0",
    "smee-client": "^1.0.2",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.1",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "\\.yaml$": "jest-raw-loader"
    }
  }
}
